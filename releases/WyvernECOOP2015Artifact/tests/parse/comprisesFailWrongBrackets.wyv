//comprises uses square brackets, not curly
tagged class Dyn {comprises DynInt, DynChar}
    class def create() : X             
        new                            
                                       
tagged class DynInt [case of Dyn]      
    class def create() : DynInt        
        new                            
                                       
tagged class DynChar [case of Dyn]     
    class def create() : DynChar       
        new                            
